<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <import resource="gs-ES5EC-data.xml"/>

    <bean id="es5ec-lab" class="org.hbird.exchange.groundstation.GroundStation">
        <constructor-arg index="0" name="ID" value="/ESTCUBE/GroundStations/ES5EC-lab" />
        <constructor-arg index="1" name="name" value="ES5EC-lab" />
        <property name="geoLocation" ref="es5ec-location" />
        <property name="version" value="1" />
    </bean>

</beans>
