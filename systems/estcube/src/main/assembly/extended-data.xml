<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
       http://www.springframework.org/schema/beans 
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context 
       http://www.springframework.org/schema/context/spring-context-3.0.xsd
       http://camel.apache.org/schema/spring 
       http://camel.apache.org/schema/spring/camel-spring.xsd">

    <bean id="estcube-lab" class="org.hbird.exchange.navigation.Satellite">
        <constructor-arg index="0" name="ID" value="/ESTCUBE/Satellites/ESTCube-1-lab" />
        <constructor-arg index="1" name="name" value="ESTCube-lab" />
        <property name="timestamp" value="1" />
        <property name="description" value="Table model of ESTCube-1" />
    </bean>

    <bean id="location-lab" class="org.hbird.exchange.navigation.GeoLocation">
        <constructor-arg index="0" name="latitude" value="58.37D"/>
        <constructor-arg index="1" name="longitude" value="26.73D"/>
        <constructor-arg index="2" name="altitude" value="59.0D"/>
    </bean>

    <!-- D3Vector geoLocation, Antenna antenna -->
    <bean id="es5ec-lab" class="org.hbird.exchange.groundstation.GroundStation">
        <constructor-arg index="0" name="ID" value="/ESTCUBE/GroundStations/ES5EC-Lab" />
        <constructor-arg index="1" name="name" value="ES5EC-Lab" />
        <property name="geoLocation" ref="location-lab" />
        <property name="timestamp" value="1" />
    </bean>


</beans>
